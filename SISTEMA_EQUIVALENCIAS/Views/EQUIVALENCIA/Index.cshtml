@model IEnumerable<SISTEMA_EQUIVALENCIAS.Models.EQUIVALENCIA>

@{
    ViewBag.Title = "Index";
}

<h2>Listado de Equivalencias</h2>

<p>
    @Html.ActionLink("Crear", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FECHA_EQUIVALENCIA)
        </th>
        <th>
            @Html.DisplayName("Carnet")
            @*@Html.DisplayNameFor(model => model.LUGAR_FISICO)*@
        </th>
        <th>
            @Html.DisplayName("Lugar Fisico")
        </th>
        <th>
            @Html.DisplayName("Sede")
        </th>
        <th>
            @Html.DisplayName("Estudiante")
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FECHA_EQUIVALENCIA)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LUGAR_FISICO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OBSERVACIONES)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SEDE.NOMBRE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ESTUDIANTE.PRIMER_NOMBRE)
            </td>
            <td>
                @Html.ActionLink("Editar", "Edit", new { id = item.ID_EQUIVALENCIA }) |
                @Html.ActionLink("Detalles", "Details", new { id = item.ID_EQUIVALENCIA }) |
                @Html.ActionLink("Remover", "Delete", new { id = item.ID_EQUIVALENCIA })
            </td>
        </tr>
    }

</table>
@Html.ActionLink("Regresar", "About", "Home")